{
  "name": "redux-express-query",
  "version": "0.0.0",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "description": "express-like middleware for your redux side-effects",
  "author": "Eric Bower",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neurosnap/redux-express-query.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "tsc && jest",
    "prepublish": "yarn build",
    "prettier-js": "prettier --write src/**/*.ts",
    "prettier-md": "prettier --config prettier.md.js --write *.md",
    "format": "yarn prettier-js && yarn prettier-md",
    "prepare": "husky install"
  },
  "dependencies": {
    "redux-batched-actions": "^0.5.0",
    "robodux": "^13.0.0"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "react-redux": ">=7.0.0",
    "redux": ">=4.0.0",
    "reselect": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-redux": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "babel-jest": "^27.4.6",
    "husky": ">=6",
    "jest": "^27.4.7",
    "lint-staged": ">=10",
    "prettier": "^2.5.1",
    "redux": "^4.1.2",
    "reselect": "^4.1.5",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.prettier-md": "prettier --write"
  }
